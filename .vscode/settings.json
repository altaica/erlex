{
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/_build/**": true
    },
    "search.exclude": {
        "**/_build/**": true,
        "**/doc/*.html": true
    }
}
